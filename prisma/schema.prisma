generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model cnae {
  codigo    String @id
  descricao String?
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model empresa {
  cnpj_basico                 String @id @unique
  razao_social                String?
  natureza_juridica           String?
  qualificacao_responsavel    String?
  capital_social              Float?
  porte_empresa               String?
  ente_federativo_responsavel String?

  estabel estabelecimento?

}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model estabelecimento {
  cnpj_basico                 String @id @unique
  cnpj_ordem                  String?
  cnpj_dv                     String?
  identificador_matriz_filial String?
  nome_fantasia               String?
  situacao_cadastral          String?
  data_situacao_cadastral     String?
  motivo_situacao_cadastral   String?
  nome_cidade_exterior        String?
  pais                        String?
  data_inicio_atividade       String?
  cnae_fiscal_principal       String?
  cnae_fiscal_secundaria      String?
  tipo_logradouro             String?
  logradouro                  String?
  numero                      String?
  complemento                 String?
  bairro                      String?
  cep                         String?
  uf                          String?
  municipio                   String
  ddd_1                       String?
  telefone_1                  String?
  ddd_2                       String?
  telefone_2                  String?
  ddd_fax                     String?
  fax                         String?
  correio_eletronico          String?
  situacao_especial           String?
  data_situacao_especial      String?

  municipalitities            munic? @relation(fields: [municipio], references: [codigo]) /// id del municipio
  company                     empresa? @relation(fields: [cnpj_basico], references: [cnpj_basico]) /// id del municipio

}


/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model moti {
  codigo    String @id @unique
  descricao String?

}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model munic {
  codigo    String @id @unique
  descricao String?

  estabel estabelecimento[]
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model natju {
  codigo    String @id @unique
  descricao String?

}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model pais {
  codigo    String @id @unique
  descricao String?

}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model quals {
  codigo    String @id @unique
  descricao String?

}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model simples {
  cnpj_basico           String @id @unique
  opcao_pelo_simples    String?
  data_opcao_simples    String?
  data_exclusao_simples String?
  opcao_mei             String?
  data_opcao_mei        String?
  data_exclusao_mei     String?

}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model socios {
  id      Int    @id @default(autoincrement())
  cnpj_basico                      String?
  identificador_socio              String?
  nome_socio_razao_social          String?
  cpf_cnpj_socio                   String?
  qualificacao_socio               String?
  data_entrada_sociedade           String?
  pais                             String?
  representante_legal              String?
  nome_do_representante            String?
  qualificacao_representante_legal String?
  faixa_etaria                     String?

}
